{
  "name": "frontend",
  "version": "0.2.0",
  "description": "Scaffold for frontend app",
  "main": "./app/index.coffee",
  "scripts": {
    "test": "./node_modules/karma/bin/karma start config/karma.conf.coffee",
    "test:watch": "npm run test -- --no-single-run",
    "build": "./node_modules/gulp/bin/gulp.js",
    "start": "npm run build -- webserver",
    "start:dev": "npm run build -- --dev"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mkyurchev/frontend.git"
  },
  "author": "Miroslav Kyurchev",
  "bugs": {
    "url": "https://github.com/mkyurchev/frontend/issues"
  },
  "homepage": "https://github.com/mkyurchev/frontend",
  "browserify": {
    "transform": [
      "coffeeify",
      "browserify-shim",
      "yamlify"
    ]
  },
  "browser": {
    "restangular": "./node_modules/restangular/dist/restangular.min.js"
  },
  "browserify-shim": {
    "restangular": {
      "depends": [
        "lodash:_"
      ]
    }
  },
  "devDependencies": {
    "angular-mocks": "^1.4.0",
    "browserify": "^10.2.0",
    "browserify-shim": "^3.8.6",
    "coffee-script": "^1.8.0",
    "coffeeify": "^1.1.0",
    "gulp": "^3.8.9",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-csscomb": "^3.0.6",
    "gulp-csso": "^1.0.0",
    "gulp-gzip": "0.0.8",
    "gulp-imagemin": "^1.2.1",
    "gulp-inject": "mkyurchev/gulp-inject#fcd2765aba",
    "gulp-minify-html": "^0.1.6",
    "gulp-ruby-sass": "^1.0.5",
    "gulp-slm": "0.0.4",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.0.1",
    "gulp-util": "^3.0.1",
    "gulp-webserver": "^0.8.4",
    "imagemin-pngquant": "^4.0.0",
    "jasmine-core": "^2.3.2",
    "karma": "^0.12.31",
    "karma-browserify": "^4.2.1",
    "karma-jasmine": "^0.3.5",
    "karma-phantomjs-launcher": "^0.1.4",
    "require-dir": "^0.3.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.0.0",
    "watchify": "^3.2.1",
    "yamlify": "^0.1.2",
    "yamljs": "^0.2.1"
  },
  "dependencies": {
    "angular": "^1.4.0",
    "angular-local-storage": "^0.2.1",
    "angular-route": "^1.4.0",
    "restangular": "^1.5.1"
  },
  "private": true
}
